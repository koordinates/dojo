if (!djConfig.isDebug) { return; }
dojo.debug('------------------------------------------------------------');
dojo.debug('Object: '+obj);
var props = [];
for(var prop in obj){
	try {
		props.push(prop + ': ' + obj[prop]);
	} catch(E) {
		props.push(prop + ': ERROR - ' + E.message);
	}
}
props.sort();
for(var i = 0; i < props.length; i++) {
	dojo.debug(props[i]);
}
dojo.debug('------------------------------------------------------------');