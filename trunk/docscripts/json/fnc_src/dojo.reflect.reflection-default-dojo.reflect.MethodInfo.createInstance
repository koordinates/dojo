switch (typeof(type)) {
	case "function" : { 
		var o = {} ;
		type.apply(o, args) ;
		return o ;
	} ;
	case "string" : {
		var o = {} ;
		(dojo.reflect.Reflector.getTypeFromString(type)).apply(o, args) ;
		return o ;
	} ;
}
throw new Error("dojo.reflect.Activator.createInstance(): no such type exists.");