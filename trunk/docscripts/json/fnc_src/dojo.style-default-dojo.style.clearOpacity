node = dojo.byId(node);
var h = dojo.render.html;
if(h.ie){
	if( node.filters && node.filters.alpha ) {
		node.style.filter = "";  FIXME: may get rid of other filter effects
	}
}else if(h.moz){
	node.style.opacity = 1;
	node.style.MozOpacity = 1;
}else if(h.safari){
	node.style.opacity = 1;
	node.style.KhtmlOpacity = 1;
}else{
	node.style.opacity = 1;
}