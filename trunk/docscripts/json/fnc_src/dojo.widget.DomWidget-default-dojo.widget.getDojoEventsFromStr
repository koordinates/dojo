var re = /(dojoOn([a-z]+)(\s?))=/gi;
var evts = str ? str.match(re)||[] : [];
var ret = [];
var lem = {};
for(var x=0; x<evts.length; x++){
	if(evts[x].legth < 1){ continue; }
	var cm = evts[x].replace(/\s/, "");
	cm = (cm.slice(0, cm.length-1));
	if(!lem[cm]){
		lem[cm] = true;
		ret.push(cm);
	}
}
return ret;