var k = model_from+'_to_'+model_to;

if (this[k]){
	return this[k](col);
}else{
	if (this.chainMaps[k]){

		var cur = model_from;
		var models = this.chainMaps[k].concat();
		models.push(model_to);

		for(var i=0; i<models.length; i++){

			col = this.convert(col, cur, models[i]);
			cur = models[i];
		}

		return col;

	}else{

		dojo.debug("Can't convert from "+model_from+' to '+model_to);
	}
}