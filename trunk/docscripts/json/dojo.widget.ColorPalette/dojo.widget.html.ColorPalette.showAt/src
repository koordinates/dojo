with(this.domNode.style){
	top = y + "px";
	left = x + "px";
	zIndex = 999;
}
document.body.appendChild(this.domNode);
if(this.bgIframe){
	with(this.bgIframe.style){
		display = "block";
		top = y + "px";
		left = x + "px";
		zIndex = 998;
		width = dojo.html.getOuterWidth(this.domNode) + "px";
		height = dojo.html.getOuterHeight(this.domNode) + "px";
	}

}