var nso = (namespaceObj || dojo.lang.anon);
if((dj_global["djConfig"])&&(djConfig["slowAnonFuncLookups"] == true)){
	for(var x in nso){
		if(nso[x] === anonFuncPtr){
			return x;
		}
	}
}
var ret = "__"+dojo.lang.anonCtr++;
while(typeof nso[ret] != "undefined"){
	ret = "__"+dojo.lang.anonCtr++;
}
nso[ret] = anonFuncPtr;
return ret;