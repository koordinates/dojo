if(!dojo.plugin._modules) {
  dojo.plugin._modules = {};
}

var pluginName = dojo.plugin._name(uri);
if (pluginName) {
  if(dojo.plugin._modules[pluginName]) {
	return 0;
  }
  var contents = dojo.hostenv.getText(uri);
  if (contents == null) {
	return "File does not exist";
  }
  contents = "dojo.plugin._plugins." + pluginName + " = {" + contents + "}";
  dj_eval(contents);  
  dojo.plugin._modules[pluginName] = pluginName;
  return 1;
}