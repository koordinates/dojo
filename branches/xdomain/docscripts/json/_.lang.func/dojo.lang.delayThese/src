if(!farr.length){ 
	if(typeof onend == "function"){
		onend();
	}
	return;
}
if((typeof delay == "undefined")&&(typeof cb == "number")){
	delay = cb;
	cb = function(){};
}else if(!cb){
	cb = function(){};
	if(!delay){ delay = 0; }
}
setTimeout(function(){
	(farr.shift())();
	cb();
	dojo.lang.delayThese(farr, cb, delay, onend);
}, delay);